{
  "name": "backend",
  "scripts": {
    "start": "ts-node-dev --no-notify --respawn --transpileOnly ./src/server",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "postinstall": "if [ -f /.dockerenv ]; then npm -s run generate; fi",
    "seed": "ts-node-dev prisma/seed.ts"
  },
  "dependencies": {
    "@nexus/schema": "0.14.0",
    "@prisma/client": "2.0.1",
    "@quixo3/prisma-session-store": "^0.2.0",
    "body-parser": "^1.19.0",
    "cuid": "^2.1.8",
    "express-session": "^1.17.0",
    "graphql": "^15.1.0",
    "graphql-yoga": "^1.18.3",
    "nexus-prisma": "0.13.0"
  },
  "devDependencies": {
    "@prisma/cli": "2.0.1",
    "@types/body-parser": "^1.19.0",
    "@types/express-session": "^1.17.0",
    "@types/node": "12.12.26",
    "ts-node": "^8.6.2",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.8.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "license": "MIT"
}
